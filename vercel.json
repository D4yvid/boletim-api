{
    "version": 2,
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/404.json"
        },
        {
            "source": "/api/v1/(.*)",
            "destination": "/src/api/v1/"
        }
    ],
    "functions": {
        "api/v1/boletim/fetch.ts": {
            "maxDuration": 60
        }
    }
}
